{
  "name": "@anakeen/admin-center",
  "version": "2.0.1",
  "autorelease": true,
  "description": "Web interfaces to admin applications and configure smart elements",
  "repository": "git@gitlab.anakeen.com:Anakeen/Platform-4/admin-center.git",
  "author": "Anakeen",
  "license": "SEE LICENSE IN LICENSE",
  "scripts": {
    "beautify": "eslint ./src --fix",
    "buildJs:tokens:prod": "element=TOKENS npx webpack --config webpack/index.js",
    "buildJs:tokens:dev": "element=TOKENS dev=DEV npx webpack --config webpack/index.js",
    "buildJs:tokens": "yarn run buildJs:tokens:prod && yarn run buildJs:tokens:dev",
    "buildJs:vaults:prod": "element=VAULTFS npx webpack --config webpack/index.js",
    "buildJs:vaults:dev": "element=VAULTFS dev=DEV npx webpack --config webpack/index.js",
    "buildJs:vaults": "yarn run buildJs:vaults:prod && yarn run buildJs:vaults:dev",
    "buildJs:parameters:prod": "element=PARAMETERS npx webpack --config webpack/index.js",
    "buildJs:parameters:dev": "element=PARAMETERS dev=DEV npx webpack --config webpack/index.js",
    "buildJs:parameters": "yarn run buildJs:parameters:prod && yarn run buildJs:parameters:dev",
    "buildJs:accounts:prod": "element=ACCOUNTS npx webpack --config webpack/index.js",
    "buildJs:accounts:dev": "element=ACCOUNTS dev=DEV npx webpack --config webpack/index.js",
    "buildJs:accounts": "yarn run buildJs:accounts:prod && yarn run buildJs:accounts:dev",
    "buildJs:i18n:prod": "element=I18N npx webpack --config webpack/index.js",
    "buildJs:i18n:dev": "element=I18N dev=DEV npx webpack --config webpack/index.js",
    "buildJs:i18n": "yarn run buildJs:i18n:prod && yarn run buildJs:i18n:dev",
    "buildJs:mail:prod": "element=MAIL npx webpack --config webpack/index.js",
    "buildJs:mail:dev": "element=MAIL dev=DEV npx webpack --config webpack/index.js",
    "buildJs:mail": "yarn run buildJs:mail:prod && yarn run buildJs:mail:dev",
    "buildJs:workflow:prod": "element=WORKFLOW npx webpack --config webpack/index.js",
    "buildJs:workflow:dev": "element=WORKFLOW dev=DEV npx webpack --config webpack/index.js",
    "buildJs:workflow": "yarn run buildJs:workflow:prod && yarn run buildJs:workflow:dev",
    "buildJs": "yarn run buildJs:tokens && yarn run buildJs:vaults && yarn run buildJs:parameters && yarn run buildJs:accounts && yarn run buildJs:i18n && yarn run buildJs:mail && yarn run buildJs:workflow"
  },
  "files": [
    "src/**/*.scss",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "@anakeen/anakeen-cli": "^1.1.13",
    "@anakeen/hub-components": "^1.0.1",
    "@anakeen/user-interfaces": "2.0.1",
    "@anakeen/webpack-conf": "^1.2.0",
    "document-register-element": "^1.13.1",
    "hard-source-webpack-plugin": "^0.13.1",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@anakeen/internal-components": "^1.0.1",
    "@progress/kendo-buttons-vue-wrapper": "^2019.2.621",
    "@progress/kendo-charts-vue-wrapper": "^2019.2.621",
    "@progress/kendo-datasource-vue-wrapper": "^2019.2.621",
    "@progress/kendo-dialog-vue-wrapper": "^2019.2.621",
    "@progress/kendo-dropdowns-vue-wrapper": "^2019.2.621",
    "@progress/kendo-gauges-vue-wrapper": "^2019.2.621",
    "@progress/kendo-grid-vue-wrapper": "^2019.2.621",
    "@progress/kendo-layout-vue-wrapper": "^2019.2.621",
    "@progress/kendo-treeview-vue-wrapper": "^2019.2.621",
    "@progress/kendo-ui": "2019.2.710",
    "@types/jquery": "^3.3.30",
    "axios": "^0.19.0",
    "core-js": "3.1.4",
    "jquery": "^3.3.1",
    "vue": "2.6.10"
  }
}
