    <sde:routes  namespace="AdminCenter">
        <sde:route name="AdminRoutes:AllRoutes">
            <sde:priority>0</sde:priority>
            <sde:callable>\Anakeen\Routes\Admin\Routes\AllRoutes</sde:callable>
