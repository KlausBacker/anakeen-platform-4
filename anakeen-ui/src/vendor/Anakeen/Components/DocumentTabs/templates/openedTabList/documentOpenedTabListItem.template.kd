<div class="documentTabs__openedTab__listItem" data-docid="#=data.initid#">
    <div class="documentTabs__openedTab__listItem__heading">
        <img class="documentTabs__openedTab__listItem__heading--icon" src="#= data.icon#"/>
        <span class="documentTabs__openedTab__listItem__heading--title">#= data.title#</span>
    </div>
    <div class="documentTabs__openedTab__listItem__close">
        <i class="material-icons">close</i>
    </div>
</div>