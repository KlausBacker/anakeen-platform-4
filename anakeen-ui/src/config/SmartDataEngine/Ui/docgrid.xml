<?xml version="1.0"?>
<sde:config xmlns:sde="https://platform.anakeen.com/4/schemas/sde/1.0">
  <sde:routes namespace="Ui:DocGrid">
    <sde:route name="Ui:DocGrid:Content">
      <sde:priority>1</sde:priority>
      <sde:callable>\Anakeen\Routes\DocumentGrid\Content</sde:callable>
      <sde:method>GET</sde:method>
      <sde:pattern>/api/v2/documentGrid/content/{collection}</sde:pattern>
      <sde:description>DocumentGrid content</sde:description>
      <sde:requiredAccess/>
    </sde:route>
    <sde:route name="Ui:DocGrid:CreateDocumentGrid">
      <sde:priority>1</sde:priority>
      <sde:callable>\Anakeen\Routes\DocumentGrid\CreateDocumentGrid</sde:callable>
      <sde:method>POST</sde:method>
      <sde:pattern>/api/v2/documentGrid/content/</sde:pattern>
      <sde:description>DocumentGrid temporary collection</sde:description>
      <sde:requiredAccess/>
    </sde:route>
    <sde:route name="Ui:DocGrid:ColumnsDefinition">
      <sde:priority>1</sde:priority>
      <sde:callable>\Anakeen\Routes\DocumentGrid\ColumnsDefinition</sde:callable>
      <sde:method>GET</sde:method>
      <sde:pattern>/api/v2/documentGrid/columnsDefinition/</sde:pattern>
      <sde:description>DocumentGrid columns definition</sde:description>
      <sde:requiredAccess/>
    </sde:route>
    <sde:route name="Ui:DocGrid:StateList">
      <sde:priority>1</sde:priority>
      <sde:callable>\Anakeen\Routes\DocumentGrid\StateList</sde:callable>
      <sde:method>GET</sde:method>
      <sde:pattern>/api/v2/documentGrid/states/{family}/</sde:pattern>
      <sde:description>DocumentGrid states list</sde:description>
      <sde:requiredAccess/>
    </sde:route>
  </sde:routes>
</sde:config>
