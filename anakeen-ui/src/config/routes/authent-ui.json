{
  "routes": {
    "Ui:Authent:Login": {
      "priority": 1,
      "callable": "\\Anakeen\\Routes\\Authent\\LoginPage",
      "methods": [
        "GET"
      ],
      "pattern": "/login/",
      "description": "Login page",
      "authenticated": false,
      "requiredAccess": null
    },
    "Ui:Authent:MailPassword": {
      "priority": 1,
      "callable": "\\Anakeen\\Routes\\Authent\\MailPassword",
      "methods": [
        "POST"
      ],
      "pattern": "/api/v2/authent/mailPassword/{userId}",
      "description": "Send email to change password",
      "requiredAccess": null,
      "authenticated": false
    },
    "Ui:Authent:Password": {
      "priority": 1,
      "callable": "\\Anakeen\\Routes\\Authent\\Password",
      "methods": [
        "PUT"
      ],
      "pattern": "/api/v2/authent/password/{login}",
      "description": "Change password",
      "requiredAccess": null,
      "authenticated": true
    }
  }
}