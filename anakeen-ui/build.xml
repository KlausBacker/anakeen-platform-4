<?xml version="1.0" ?>
<module:config xmlns:module="https://platform.anakeen.com/4/schemas/module/1.0">
    <module:source path="src" />
    <module:po-config>
        <module:po-struct source="**/*struct.xml"/>
        <module:po-enum source="**/*struct.xml"/>
        <module:po-mustache target="user-interfaces" source="**/*.mustache"/>
        <module:po-php source="**/*.php" target="user-interfaces"/>
        <module:po-js source="Apps/DOCUMENT/**/*js" target="DOCUMENT"/>
        <module:po-js source="vendor/Anakeen/SmartStructures/Dsearch/**/*js" target="SEARCH_UI_HTML5"/>
    </module:po-config>
    <module:stub-config>
        <module:stub-struct source="**/*struct.xml"/>
    </module:stub-config>
    <module:check-config>
        <module:config-xml source="**/*.xml"/>
    </module:check-config>
</module:config>