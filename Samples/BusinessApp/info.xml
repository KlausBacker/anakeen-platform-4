<?xml version="1.0" ?>
<module xmlns="urn:dynacase:webinst:module/1.0" name="@moduleName@"
        license="http://docs.anakeen.com/licenses/anakeen-3.0/license.html Anakeen License Agreement v3"
        vendor="Anakeen" release="@release@" version="@version@">
    <description>Sample Business App</description>

    <requires>
        <module name="user-interfaces"/>
    </requires>
    <replaces>
        <module name="sample-BusinessApp"/>
        <module name="sample-business-app"/>
    </replaces>
    <post-install>
        <process command="./wsh.php --app=HTTPAPI_V1 --action=INIT_RULES"/>
        <process command="programs/record_application BUSINESS_APP"/>

        <process command="./wsh.php --api=importDocuments --file=./Apps/BUSINESS_APP/Families/ClientContract/client.struct.csv --csv-enclosure='&quot;'" />
        <process command="./wsh.php --api=importDocuments --file=./Apps/BUSINESS_APP/Families/ClientContract/client.param.csv --csv-enclosure='&quot;'" />
        <process command="./wsh.php --api=importDocuments --file=./Apps/BUSINESS_APP/Families/ProviderContract/provider.struct.csv --csv-enclosure='&quot;'" />
        <process command="./wsh.php --api=importDocuments --file=./Apps/BUSINESS_APP/Families/ProviderContract/provider.param.csv --csv-enclosure='&quot;'" />
        <process command="./wsh.php --api=importDocuments --file=./Apps/BUSINESS_APP/Families/RHDirectory/rhdirectory.struct.csv --csv-enclosure='&quot;'" />
        <process command="./wsh.php --api=importDocuments --file=./Apps/BUSINESS_APP/Families/Fees/fees.struct.csv --csv-enclosure='&quot;'" />
        <process command="./wsh.php --api=importDocuments --file=./Apps/BUSINESS_APP/Families/Fees/fees.param.csv --csv-enclosure='&quot;'" />
        <process command="./wsh.php --api=importDocuments --file=./Apps/BUSINESS_APP/Families/Fees/fees.workflow.csv --csv-enclosure='&quot;'" />
        <process command="./wsh.php --api=importDocuments --file=./Apps/BUSINESS_APP/Families/Fees/BA_FEES_WORKFLOW.csv --csv-enclosure='&quot;'" />
        <process command="./wsh.php --api=importDocuments --file=./Apps/BUSINESS_APP/Families/Categories/categories.struct.csv --csv-enclosure='&quot;'" />
        <process command="./wsh.php --api=importDocuments --file=./Apps/BUSINESS_APP/Families/Categories/categories.param.csv --csv-enclosure='&quot;'" />
        <process command="./wsh.php --api=importDocuments --file=./Apps/BUSINESS_APP/Families/Categories/BA_CATEGORIES.csv --csv-enclosure='&quot;'" />
        <process command='./wsh.php --api=importAccounts --file=./Apps/BUSINESS_APP/Families/Iuser/accounts.xml'/>

        <process command="./wsh.php --app=HTTPAPI_V1 --action=INIT_RULES"/>
        <process command="./wsh.php --app=DOCUMENT --action=LOAD_RENDERPARAMETERS"/>
        <process command="./wsh.php --api=setApplicationParameter --appname=CORE --param=CORE_START_APP --value=BUSINESS_APP"/>
        <process command="programs/update_catalog"/>
    </post-install>
    <post-upgrade>
        <process command="./wsh.php --app=HTTPAPI_V1 --action=INIT_RULES"/>
        <process command="programs/pre_migration BUSINESS_APP"/>
        <process command="programs/record_application BUSINESS_APP"/>
        <process command="programs/post_migration BUSINESS_APP"/>

        <process command="./wsh.php --api=importDocuments --file=./Apps/BUSINESS_APP/Families/ClientContract/client.struct.csv --csv-enclosure='&quot;'" />
        <process command="./wsh.php --api=importDocuments --file=./Apps/BUSINESS_APP/Families/ClientContract/client.param.csv --csv-enclosure='&quot;'" />
        <process command="./wsh.php --api=importDocuments --file=./Apps/BUSINESS_APP/Families/ProviderContract/provider.struct.csv --csv-enclosure='&quot;'" />
        <process command="./wsh.php --api=importDocuments --file=./Apps/BUSINESS_APP/Families/ProviderContract/provider.param.csv --csv-enclosure='&quot;'" />
        <process command="./wsh.php --api=importDocuments --file=./Apps/BUSINESS_APP/Families/RHDirectory/rhdirectory.struct.csv --csv-enclosure='&quot;'" />
        <process command="./wsh.php --api=importDocuments --file=./Apps/BUSINESS_APP/Families/Fees/fees.struct.csv --csv-enclosure='&quot;'" />
        <process command="./wsh.php --api=importDocuments --file=./Apps/BUSINESS_APP/Families/Fees/fees.param.csv --csv-enclosure='&quot;'" />
        <process command="./wsh.php --api=importDocuments --file=./Apps/BUSINESS_APP/Families/Fees/fees.workflow.csv --csv-enclosure='&quot;'" />
        <process command="./wsh.php --api=importDocuments --file=./Apps/BUSINESS_APP/Families/Categories/categories.struct.csv --csv-enclosure='&quot;'" />
        <process command="./wsh.php --api=importDocuments --file=./Apps/BUSINESS_APP/Families/Categories/categories.param.csv --csv-enclosure='&quot;'" />
        <process command='./wsh.php --api=importAccounts --file=./Apps/BUSINESS_APP/Families/Iuser/accounts.xml'/>

        <process command="./wsh.php --app=HTTPAPI_V1 --action=INIT_RULES"/>
        <process command="./wsh.php --app=DOCUMENT --action=LOAD_RENDERPARAMETERS"/>
        <process command="programs/update_catalog"/>
    </post-upgrade>
</module>
