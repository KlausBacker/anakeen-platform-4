<?xml version="1.0" ?>
<module xmlns="urn:dynacase:webinst:module/1.0" name="@moduleName@" release="@release@" version="@version@">
    <description>Sample Business App</description>

    <requires>
        <module name="anakeen-ui"/>
    </requires>
    <replaces>
        <module name="sample-BusinessApp"/>
    </replaces>
    <post-install>
        <process command="./wsh.php --app=HTTPAPI_V1 --action=INIT_RULES"/>
        <process command="programs/record_application BUSINESS_APP"/>

        <process command="./wsh.php --api=importDocuments --file=./Apps/BUSINESS_APP/Families/ContratClient/contratclient.struct.csv --csv-enclosure='&quot;'" />
        <process command="./wsh.php --api=importDocuments --file=./Apps/BUSINESS_APP/Families/ContratFournisseur/contratfournisseur.struct.csv --csv-enclosure='&quot;'" />
        <process command="./wsh.php --api=importDocuments --file=./Apps/BUSINESS_APP/Families/DossierRH/dossierrh.struct.csv --csv-enclosure='&quot;'" />
        <process command="./wsh.php --api=importDocuments --file=./Apps/BUSINESS_APP/Families/NoteFrais/notefrais.struct.csv --csv-enclosure='&quot;'" />
        <process command="./wsh.php --api=importDocuments --file=./Apps/BUSINESS_APP/Families/Laboratory/laboratory.struct.csv --csv-enclosure='&quot;'" />
        <process command="./wsh.php --api=importDocuments --file=./Apps/BUSINESS_APP/Families/Prospect/prospect.struct.csv --csv-enclosure='&quot;'" />
        <process command="./wsh.php --api=importDocuments --file=./Apps/BUSINESS_APP/Families/Prospect/prospect.param.csv --csv-enclosure='&quot;'" />
        <process command="./wsh.php --api=importDocuments --file=./Apps/BUSINESS_APP/Families/Client/client.struct.csv --csv-enclosure='&quot;'" />
        <process command="./wsh.php --api=importDocuments --file=./Apps/BUSINESS_APP/Families/Certification/certification.struct.csv --csv-enclosure='&quot;'" />
        <process command="./wsh.php --api=importDocuments --file=./Apps/BUSINESS_APP/Families/Certification/certification.workflow.csv --csv-enclosure='&quot;'" />
        <process command="./wsh.php --api=importDocuments --file=./Apps/BUSINESS_APP/Families/Certification/BA_CERTIFICATION-exports.csv --csv-enclosure='&quot;'" />
        <process command="./wsh.php --api=importDocuments --file=./Apps/BUSINESS_APP/Families/Client/BA_CLIENT-exports.csv --csv-enclosure='&quot;'" />
        <process command="./wsh.php --api=importDocuments --file=./Apps/BUSINESS_APP/Families/Prospect/BA_PROSPECT-exports.csv --csv-enclosure='&quot;'" />

        <process command="./wsh.php --app=HTTPAPI_V1 --action=INIT_RULES"/>
        <process command="./wsh.php --app=DOCUMENT --action=LOAD_RENDERPARAMETERS"/>
        <process command="./wsh.php --api=setApplicationParameter --appname=CORE --param=CORE_START_APP --value=BUSINESS_APP"/>
        <process command="programs/update_catalog"/>
    </post-install>
    <post-upgrade>
        <process command="./wsh.php --app=HTTPAPI_V1 --action=INIT_RULES"/>
        <process command="programs/pre_migration BUSINESS_APP"/>
        <process command="programs/record_application BUSINESS_APP"/>
        <process command="programs/post_migration BUSINESS_APP"/>

        <process command="./wsh.php --api=importDocuments --file=./Apps/BUSINESS_APP/Families/ContratClient/contratclient.struct.csv --csv-enclosure='&quot;'" />
        <process command="./wsh.php --api=importDocuments --file=./Apps/BUSINESS_APP/Families/ContratFournisseur/contratfournisseur.struct.csv --csv-enclosure='&quot;'" />
        <process command="./wsh.php --api=importDocuments --file=./Apps/BUSINESS_APP/Families/DossierRH/dossierrh.struct.csv --csv-enclosure='&quot;'" />
        <process command="./wsh.php --api=importDocuments --file=./Apps/BUSINESS_APP/Families/NoteFrais/notefrais.struct.csv --csv-enclosure='&quot;'" />
        <process command="./wsh.php --api=importDocuments --file=./Apps/BUSINESS_APP/Families/Laboratory/laboratory.struct.csv --csv-enclosure='&quot;'" />
        <process command="./wsh.php --api=importDocuments --file=./Apps/BUSINESS_APP/Families/Prospect/prospect.struct.csv --csv-enclosure='&quot;'" />
        <process command="./wsh.php --api=importDocuments --file=./Apps/BUSINESS_APP/Families/Client/client.struct.csv --csv-enclosure='&quot;'" />
        <process command="./wsh.php --api=importDocuments --file=./Apps/BUSINESS_APP/Families/Certification/certification.struct.csv --csv-enclosure='&quot;'" />
        <process command="./wsh.php --api=importDocuments --file=./Apps/BUSINESS_APP/Families/Certification/certification.workflow.csv --csv-enclosure='&quot;'" />

        <process command="./wsh.php --app=HTTPAPI_V1 --action=INIT_RULES"/>
        <process command="./wsh.php --app=DOCUMENT --action=LOAD_RENDERPARAMETERS"/>
        <process command="programs/update_catalog"/>
    </post-upgrade>
</module>
