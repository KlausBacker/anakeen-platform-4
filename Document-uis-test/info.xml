<?xml version="1.0"?>
<module xmlns="urn:dynacase:webinst:module/1.0"  name="@moduleName@" release="@release@" version="@version@"  license="http://docs.anakeen.com/licenses/anakeen-3.0/license.html Anakeen License Agreement v3" >

    <description>Document UIS auto-test</description>

    <requires>
        <module name="dynacase-core"/>
        <module name='dynacase-document-uis'/>
        <module name='dynacase-jasmine-installer'/>
    </requires>

    <post-install>
        <process command="programs/record_application TEST_DOCUMENT"/>
        <process command="programs/update_catalog"/>
    </post-install>

    <post-upgrade>
        <process command="programs/pre_migration TEST_DOCUMENT"/>
        <process command="programs/record_application TEST_DOCUMENT"/>
        <process command="programs/post_migration TEST_DOCUMENT"/>
        <process command="programs/update_catalog"/>
    </post-upgrade>

</module>
