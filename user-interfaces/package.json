{
  "name": "@anakeen/user-interfaces",
  "version": "3.0.2-RC.2020.01.2",
  "autorelease": true,
  "description": "A package for the user interface of anakeen-platform",
  "keywords": [
    "anakeen-platform",
    "smart element",
    "user interface",
    "component"
  ],
  "scripts": {
    "beautify": "npx eslint ./src --fix",
    "lib:rollup": "CLEAN=true npx rollup -c ./components/rollup/rollup.config.js",
    "buildLogin:prod": "conf=PROD npx webpack --config ./webpackConfig/login.js",
    "buildLogin:dev": "conf=DEV npx webpack --config ./webpackConfig/login.js",
    "buildLogin": "yarn run buildLogin:prod && yarn run buildLogin:dev",
    "buildSmartElement:prod": "conf=PROD npx webpack --config ./webpackConfig/smartElement.js",
    "buildSmartElement:dev": "conf=DEV npx webpack --config ./webpackConfig/smartElement.js",
    "buildSmartElement": "yarn run buildSmartElement:prod && yarn run buildSmartElement:dev",
    "buildSmartStructures:prod": "conf=PROD npx webpack --config ./webpackConfig/smartStructures.js",
    "buildSmartStructures:dev": "conf=DEV npx webpack --config ./webpackConfig/smartStructures.js",
    "buildSmartStructures": "yarn run buildSmartStructures:prod && yarn run buildSmartStructures:dev",
    "buildTest:prod": "conf=PROD npx webpack --config ./webpackConfig/test.js",
    "buildTest:dev": "conf=DEV npx webpack --config ./webpackConfig/test.js",
    "buildTest": "yarn run buildTest:prod && yarn run buildTest:dev",
    "buildAsset:dev": "conf=DEV npx webpack --config ./webpackConfig/assets.js",
    "buildAsset": "yarn run buildAsset:dev",
    "buildCss": " npx webpack --config ./webpackConfig/buildCss.js",
    "lint": "npx eslint ./src"
  },
  "files": [
    "constants/*.js",
    "components/**/*",
    "src/**/*.scss",
    "src/public/Anakeen/ank-components/deps/*",
    "src/public/Anakeen/assets/**/*.json",
    "src/public/Anakeen/ankDll/**/*.json",
    "src/public/Anakeen/manifest/**/*.json",
    "src/vendor/Anakeen/DOCUMENT/**/*",
    "LICENSE",
    "README.md",
    "yarn.lock"
  ],
  "author": "Anakeen",
  "license": "SEE LICENSE IN LICENSE",
  "repository": "git@gitlab.anakeen.com:Anakeen/Platform-4/user-interfaces.git",
  "devDependencies": {
    "@anakeen/anakeen-cli": "1.2.2-RC.2020.01.2",
    "@anakeen/webpack-conf": "1.2.1",
    "core-js": "3.6.1",
    "@types/backbone": "^1.4.0",
    "@types/jquery": "^3.3.29",
    "@types/kendo-ui": "^2019.3.1",
    "@yelo/rollup-node-external": "^1.0.1",
    "copy-webpack-plugin": "^5.1.1",
    "expose-loader": "^0.7.5",
    "hard-source-webpack-plugin": "^0.13.1",
    "node-sass-magic-importer": "^5.3.2",
    "postcss": "7.0.26",
    "rollup": "^1.27.14",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-progress": "^1.1.1",
    "rollup-plugin-scss": "^2.1.0",
    "rollup-plugin-typescript2": "^0.25.3",
    "rollup-plugin-vue": "^5.1.4",
    "v-jsoneditor": "^1.2.3",
    "vue-m-loader": "^1.0.1",
    "webpack": "^4.0.0",
    "webpack-node-externals": "^1.7.2"
  },
  "peerDependencies": {
    "@progress/kendo-theme-bootstrap": "4.8.0",
    "@progress/kendo-ui": "2019.3.1113",
    "jquery": "3.4.1",
    "vue": "2.6.11"
  },
  "dependencies": {
    "@anakeen/theme-variables": "1.0.0-RC.2020.01.2",
    "@progress/kendo-theme-bootstrap": "4.8.0",
    "@progress/kendo-ui": "2019.3.1113",
    "axios": "^0.19.0",
    "backbone": "^1.4.0",
    "bootstrap": "^4.4.1",
    "ckeditor4": "^4.13.1",
    "datatables.net": "^1.10.20",
    "datatables.net-bs4": "^1.10.20",
    "easygettext": "^2.9.0",
    "jquery": "3.4.1",
    "little-loader": "^0.2.0",
    "load-css-file": "^1.0.0",
    "mustache": "^3.2.1",
    "popper.js": "^1.16.0",
    "tracekit": "^0.4.5",
    "underscore": "^1.8.3",
    "vue": "2.6.11",
    "vue-i18n": "^8.15.3",
    "vue-property-decorator": "^8.3.0",
    "vue-runtime-helpers": "^1.1.2",
    "vue-template-compiler": "2.6.11"
  }
}
