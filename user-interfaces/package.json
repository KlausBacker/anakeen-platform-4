{
  "name": "@anakeen/user-interfaces",
  "version": "3.0.0",
  "autorelease": true,
  "description": "A package for the user interface of anakeen-platform",
  "keywords": [
    "anakeen-platform",
    "smart element",
    "user interface",
    "component"
  ],
  "main": "./components/lib/ankComponents.esm.js",
  "module": "./components/lib/ankComponents.esm.js",
  "types": "./components/lib/types/index.d.ts",
  "publishConfig": {
    "registry": "http://localhost:4873"
  },
  "scripts": {
    "beautify": "npx eslint ./src --fix",
    "lib:rollup": "npx rollup -c ./components/rollup/rollup.config.js",
    "lib:dev": "npx webpack --env development --config ./components/webpack/index.js",
    "lib:controller": "conf=CONTROLLER npx webpack --env development --config ./components/webpack/index.js",
    "lib": "yarn run lib:controller && yarn run lib:dev",
    "buildComponent:prod": "conf=PROD npx webpack --config ./webpackConfig/components.js",
    "buildComponent:dev": "conf=DEV npx webpack --config ./webpackConfig/components.js",
    "buildComponent:legacy": "conf=LEGACY npx webpack --config ./webpackConfig/components.js",
    "buildComponent": "yarn run buildComponent:prod && yarn run buildComponent:dev && yarn run buildComponent:legacy",
    "buildLogin:prod": "conf=PROD npx webpack --config ./webpackConfig/login.js",
    "buildLogin:dev": "conf=DEV npx webpack --config ./webpackConfig/login.js",
    "buildLogin:legacy": "conf=LEGACY npx webpack --config ./webpackConfig/login.js",
    "buildLogin": "yarn run buildLogin:prod && yarn run buildLogin:dev && yarn run buildLogin:legacy",
    "buildSmartElement:prod": "conf=PROD npx webpack --config ./webpackConfig/smartElement.js",
    "buildSmartElement:dev": "conf=DEV npx webpack --config ./webpackConfig/smartElement.js",
    "buildSmartElement:legacy": "conf=LEGACY npx webpack --config ./webpackConfig/smartElement.js",
    "buildSmartElement": "yarn run buildSmartElement:prod && yarn run buildSmartElement:dev && yarn run buildSmartElement:legacy",
    "buildSmartStructures:prod": "conf=PROD npx webpack --config ./webpackConfig/smartStructures.js",
    "buildSmartStructures:dev": "conf=DEV npx webpack --config ./webpackConfig/smartStructures.js",
    "buildSmartStructures:legacy": "conf=LEGACY npx webpack --config ./webpackConfig/smartStructures.js",
    "buildSmartStructures": "yarn run buildSmartStructures:prod && yarn run buildSmartStructures:dev && yarn run buildSmartStructures:legacy",
    "buildTest:prod": "conf=PROD npx webpack --config ./webpackConfig/test.js",
    "buildTest:dev": "conf=DEV npx webpack --config ./webpackConfig/test.js",
    "buildTest:legacy": "conf=LEGACY npx webpack --config ./webpackConfig/test.js",
    "buildTest": "yarn run buildTest:prod && yarn run buildTest:dev && yarn run buildTest:legacy",
    "buildAsset:dev": "conf=DEV npx webpack --config ./webpackConfig/assets.js",
    "buildAsset:legacy": "conf=LEGACY npx webpack --config ./webpackConfig/assets.js",
    "buildAsset": "yarn run buildAsset:dev && yarn run buildAsset:legacy",
    "buildCss": " npx webpack --config ./webpackConfig/buildCss.js",
    "buildDll:dev": "conf=DEV npx webpack --config ./webpackConfig/dll.js",
    "buildDll:legacy": "conf=LEGACY npx webpack --config ./webpackConfig/dll.js",
    "buildDll": "yarn run buildDll:dev && yarn run buildDll:legacy",
    "buildPolyfill": "conf=LEGACY npx webpack --config ./webpackConfig/polyfill.js",
    "lint": "npx eslint ./src"
  },
  "files": [
    "lib/**/*",
    "constants/*.js",
    "components/**/*",
    "src/**/*.scss",
    "src/public/Anakeen/ank-components/deps/*",
    "src/public/Anakeen/assets/**/*.json",
    "src/public/Anakeen/ankDll/**/*.json",
    "src/public/Anakeen/manifest/**/*.json",
    "src/vendor/Anakeen/DOCUMENT/**/*",
    "LICENSE",
    "README.md",
    "yarn.lock"
  ],
  "author": "Anakeen",
  "license": "SEE LICENSE IN LICENSE",
  "repository": "git@gitlab.anakeen.com:Anakeen/Platform-4/user-interfaces.git",
  "devDependencies": {
    "@anakeen/anakeen-cli": "1.2.0",
    "@anakeen/webpack-conf": "1.2.1",
    "@types/backbone": "^1.4.0",
    "@types/jquery": "^3.3.29",
    "@types/kendo-ui": "^2019.1.1",
    "@yelo/rollup-node-external": "^1.0.1",
    "copy-webpack-plugin": "^5.0.2",
    "expose-loader": "^0.7.5",
    "hard-source-webpack-plugin": "^0.13.1",
    "node-sass-magic-importer": "^5.3.2",
    "rollup": "^1.23.1",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-progress": "^1.1.1",
    "rollup-plugin-scss": "^1.0.2",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-typescript2": "^0.25.2",
    "rollup-plugin-visualizer": "^2.6.0",
    "rollup-plugin-vue": "^5.0.1",
    "tslib": "^1.10.0",
    "v-jsoneditor": "^1.2.1",
    "vue-m-loader": "^1.0.1",
    "webpack-node-externals": "^1.7.2"
  },
  "peerDependencies": {
    "@progress/kendo-ui": "2019.3.917",
    "jquery": "3.4.1",
    "vue": "2.6.10"
  },
  "dependencies": {
    "@anakeen/theme-variables": "1.0.0",
    "@progress/kendo-theme-bootstrap": "^4.0.0",
    "@progress/kendo-ui": "^2019.3.917",
    "axios": "^0.19.0",
    "backbone": "^1.4.0",
    "bootstrap": "^4.3.1",
    "ckeditor4": "^4.12.1",
    "core-js": "3.1.4",
    "datatables.net": "^1.10.16",
    "datatables.net-bs4": "^1.10.16",
    "easygettext": "^2.7.0",
    "jquery": "^3.4.1",
    "little-loader": "^0.2.0",
    "load-css-file": "^1.0.0",
    "mustache": "^3.0.1",
    "popper.js": "^1.14.7",
    "tracekit": "^0.4.5",
    "typescript": "^3.6.4",
    "underscore": "^1.8.3",
    "vue": "2.6.10",
    "vue-i18n": "^8.14.1",
    "vue-property-decorator": "^8.2.2",
    "vue-runtime-helpers": "^1.1.2",
    "vue-template-compiler": "2.6.10",
    "whatwg-fetch": "^3.0.0"
  }
}
