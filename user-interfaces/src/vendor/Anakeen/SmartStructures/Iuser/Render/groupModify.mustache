<div class="igroup-changegroup">
    <div class="igroup-list igroup-ag">
        <h3>Available groups</h3>
        <script type="text/x-kendo-template" name="availableGroupTemplate">
            <div data-id="#:accountId#" class="igroup-item add-group #:tag#">
                <span>#:lastname#</span>
                <button class="btn btn-primary add-group"> &gt;&gt;</button>
            </div>
        </script>
        <form name="availableGroupForm">
            <input type="search" class="form-control"  name="availableGroupSearch" placeholder="Search..."/>
        </form>
        <div name="availableGroupList" data-filter="not"/>
        <div name="availableGroupPager"/>
    </div>

    <div class="igroup-list igroup-pg">
        <h3>Parent groups</h3>
        <script type="text/x-kendo-template" name="parentGroupTemplate">
            <div data-id="#:accountId#" class="igroup-item delete-group #:tag#">
                <button  class="btn btn-primary delete-group"> &lt;&lt;</button>
                <span>#:lastname#</span>
            </div>
        </script>
        <form name="parentGroupForm">
            <input type="search" class="form-control"  name="parentGroupSearch" placeholder="Search..."/>
        </form>
        <div name="parentGroupList"/>
        <div name="parentGroupPager"/>
    </div>
</div>