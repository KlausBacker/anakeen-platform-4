<li data-menu-id="{{id}}" data-menu-url="{{url}}"
    {{# htmlAttr}}{{attrId}}="{{attrValue}}"{{/ htmlAttr}}
    {{# disabled}}disabled="disabled"{{/ disabled}}
    {{# tooltipLabel}}title="{{tooltipLabel}}"{{/tooltipLabel}}
    class="menu__element menu__element--dynamic {{cssClass}}">
    {{#iconUrl}}
    <img src="{{iconUrl}}" class="k-image" />
    {{/iconUrl}}
    {{#beforeContent}}
    <span class="menu__before-content k-image">
        {{{beforeContent}}}
    </span>
    {{/beforeContent}}
    {{#contentLabel}}<span class="menu__content">{{{htmlLabel}}}{{label}}</span>{{/contentLabel}}
    <ul class="listmenu__content">
       <li class="menu__loading">Loading...</li>
    </ul>
</li>