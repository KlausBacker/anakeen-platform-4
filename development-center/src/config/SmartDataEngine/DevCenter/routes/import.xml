<?xml version="1.0"?>
<sde:config xmlns:sde="https://platform.anakeen.com/4/schemas/sde/1.0">
    <sde:routes namespace="DevelopmentCenter">
        <sde:route name="importConfiguration">
            <sde:callable>Anakeen\Routes\Devel\Import\Configuration</sde:callable>
            <sde:method>POST</sde:method>
            <sde:pattern>/api/v2/devel/import/configuration/</sde:pattern>
            <sde:description>Import a configuration</sde:description>
            <sde:requiredAccess>
                <sde:access ns="DevCenter">DevelAccess</sde:access>
            </sde:requiredAccess>
        </sde:route>
        <sde:route name="includeFiles">
            <sde:callable>Anakeen\Routes\Devel\Import\IncludeFiles</sde:callable>
            <sde:method>POST</sde:method>
            <sde:pattern>/api/v2/devel/import/include-files/</sde:pattern>
            <sde:description>Get Zip included files</sde:description>
        </sde:route>
    </sde:routes>
</sde:config>