<?xml version="1.0" encoding="utf-8"?>
<accounts>
    <roles>
        <role>
            <reference>hub_user_role</reference>
            <displayName>Hub users</displayName>
            <document family="ROLE">
                <role  name="hub-user-role"/>
            </document>
        </role>
        <role>
            <reference>hub_admin_role</reference>
            <displayName>Hub admin</displayName>
            <document family="ROLE">
                <role  name="hub-admin-role"/>
            </document>
        </role>
    </roles>
    <groups>
        <group>
            <reference>all</reference>
            <displayName>Main Group</displayName>
            <associatedRoles reset="false">
                <associatedRole reference="hub_user_role"/>
            </associatedRoles>
        </group>
        <group>
            <reference>gadmin</reference>
            <displayName>Administrators</displayName>
            <associatedRoles reset="false">
                <associatedRole reference="hub_admin_role"/>
            </associatedRoles>
        </group>
    </groups>
</accounts>