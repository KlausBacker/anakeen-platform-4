<?xml version="1.0" encoding="utf-8"?>
<accounts:accounts xmlns:accounts="https://platform.anakeen.com/4/schemas/account/1.0">
    <accounts:roles>
        <accounts:role name="hub-user-role">
            <accounts:displayName>Hub users</accounts:displayName>
        </accounts:role>

        <accounts:role name="hub-admin-role">
            <accounts:displayName>Hub admin</accounts:displayName>
        </accounts:role>
    </accounts:roles>

    <accounts:groups>
        <accounts:group name="all">
            <accounts:associatedRoles>
                <accounts:associatedRole ref="hub-user-role"/>
            </accounts:associatedRoles>
        </accounts:group>

        <accounts:group name="gadmin">
            <accounts:associatedRoles>
                <accounts:associatedRole ref="hub-admin-role"/>
            </accounts:associatedRoles>
        </accounts:group>
    </accounts:groups>
</accounts:accounts>