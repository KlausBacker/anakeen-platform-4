<html>
    <head>
        <title>[app_short_name]</title>
        [CSS:CUSTOMREF]
        [BLOCK JS_LINKS]
        <script type="text/javascript" src="[src]"></script>
        [ENDBLOCK JS_LINKS]
    </head>
    <body>
        <table align="center" width="100%" border="0" cellspacing="0"
               cellpadding="0" class="title">
            <tr>
                <td class="CORETblHeadCell">
                    <input type="hidden" id="searchId" value="[action_appl_id]"/>
                    <input type="text" id="id" value="[id]"/>
                </td>
            </tr>
        </table>

        <table align="center" width="100%" border="0" cellspacing="0"
               cellpadding="0" id="dataTable">
            <thead>
                <!-- TABLEHEAD -->
                <tr>
                    <th class="ui-widget-header name">
                        <span class="CORETblBold">[TEXT:name]</span>
                    </th>
                    <th class="ui-widget-header short_name"
                        style="white-space: nowrap">
                        <span class="CORETblBold">[TEXT:appmng:short_name]</span>
                    </th>
                    <th class="ui-widget-header available">
                        <span class="CORETblBold">[TEXT:available]</span>
                    </th>
                    <th class="ui-widget-header acl">
                        <span class="CORETblBold">[TEXT:acl]</span>
                    </th>
                    <th class="ui-widget-header openaccess">
                        <span class="CORETblBold">[TEXT:openaccess]</span>
                    </th>
                    <th class="ui-widget-header root">
                        <span class="CORETblBold">[TEXT:root]</span>
                    </th>
                </tr>
                <!-- /TABLEHEAD -->
            </thead>


            <tbody>

            </tbody>

        </table>
    </body>
</html>
