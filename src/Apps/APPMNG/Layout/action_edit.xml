<form name="edit">

<input type="hidden" name="id" value="[id]"/>

  <table align="center"  border="0" cellspacing="0" cellpadding="2">

  <tr>
    <td valign="center" align="right" class="CORETblCell"><span class="CORETbl">[TEXT:name] : </span></td>
    <td colspan="3" valign="center" align="left" class="CORETblCell"><span class="CORETbl">
      <font size="-1">[name]</font>
    </span></td>
  </tr>

  <tr>
    <td valign="center" align="right" class="CORETblCell"><span class="CORETbl">[TEXT:appmng:short_name] : </span></td>
    <td colspan="3" valign="center" align="left" class="CORETblCell"><span class="CORETbl">
      <font size="-1">[short_name]</font>
    </span></td>
  </tr>

  <tr>
    <td valign="center" align="right" class="CORETblCell"><span class="CORETbl">[TEXT:appmng:long_name] : </span></td>
    <td colspan="3" valign="center" align="left" class="CORETblCell"><span class="CORETbl">
      <font size="-1">[long_name]</font>
    </span></td>
  </tr>

  <tr>
    <td valign="center" align="right" class="CORETblCell"><span class="CORETbl">[TEXT:available] : </span></td>
    <td colspan="1" valign="center" align="left" class="CORETblCell"><span class="CORETbl">
        <input type="hidden" name="available" value="[select_available]" id="input_select"/>
        <button id="select" class="select" data-menu="menuSelectable">[select_available]</button>
    </span></td>

    <td valign="center" align="right" class="CORETblCell"><span class="CORETbl">[TEXT:acl] : </span></td>
    <td colspan="1" valign="center" align="left" class="CORETblCell"><span class="CORETbl">
      <font size="-1">[acl]</font>
    </span></td>
  </tr>

  <tr>
    <td valign="center" align="right" class="CORETblCell"><span class="CORETbl">[TEXT:root] : </span></td>
    <td colspan="1" valign="center" align="left" class="CORETblCell"><span class="CORETbl">
      <font size="-1">[root]</font>
    </span></td>

    <td valign="center" align="right" class="CORETblCell"><span class="CORETbl">[TEXT:toc] : </span></td>
    <td colspan="1" valign="center" align="left" class="CORETblCell"><span class="CORETbl">
      <font size="-1">[toc]</font>
    </span></td>
  </tr>
 <tr>
    <td valign="center" align="right" class="CORETblCell"><span class="CORETbl">[TEXT:openaccess] : </span></td>
    <td colspan="1" valign="center" align="left" class="CORETblCell"><span class="CORETbl">
      <font size="-1">[openaccess]</font>
    </span></td>
  </tr>

  </table>
    <ul id="menuSelectable" class="menuSelectable" data-button="select">
        [BLOCK SELECTAVAILABLE]

          <li><a href="#">[available]</a></li>

        [ENDBLOCK SELECTAVAILABLE]

      </ul>
</form>
