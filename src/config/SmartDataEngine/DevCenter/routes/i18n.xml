<?xml version="1.0"?>
<sde:config xmlns:sde="https://platform.anakeen.com/4/schemas/sde/1.0">
    <sde:routes namespace="DevelopmentCenter">
        <sde:route name="getI18n">
            <sde:callable>Anakeen\Routes\Devel\I18n</sde:callable>
            <sde:method>GET</sde:method>
            <sde:pattern>/api/v2/devel/i18n/</sde:pattern>
            <sde:description>Get all locale recorded in server</sde:description>
            <sde:requiredAccess>
                <sde:access ns="DevCenter">DevelAccess</sde:access>
            </sde:requiredAccess>
        </sde:route>
    </sde:routes>
</sde:config>