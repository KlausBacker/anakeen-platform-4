            "applicationContext": "UITEST",
            "description": "Fake Normal Access"
        },
        "fake:admin": {
            "description": "Fake Administrator Access"
        }
    },
    "middlewares": {
        "Core:Middleware:FakeV2": {
            "priority": 10,
            "callable": "\\Anakeen\\Routes\\Middleware\\Log::Write",
