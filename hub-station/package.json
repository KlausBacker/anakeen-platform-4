    "@progress/kendo-dropdowns-vue-wrapper": "^2019.3.1023",
    "@progress/kendo-layout-vue-wrapper": "^2019.3.1023",
    "@progress/kendo-theme-bootstrap": "4.8.0",
    "@progress/kendo-ui": "2019.3.1113",
    "@types/navigo": "^7.0.1",
    "@types/url-join": "^4.0.0",
    "axios": "0.19.0",
    "fg-loadcss": "^2.1.0",
    "jquery": "3.4.1",
    "little-loader": "^0.2.0",
    "navigo": "^7.1.2",
    "url-join": "^4.0.1",
    "uuid": "^3.3.3",
    "vue": "2.6.11",
    "vue-class-component": "7.1.0",
    "vue-property-decorator": "8.3.0",
    "vuebar": "^0.0.20",
    "vuex": "3.1.2"
  },
  "devDependencies": {
    "@anakeen/anakeen-cli": "1.2.2-RC.2020.1.21",
    "@anakeen/internal-components": "1.0.3-RC.2020.1.21",
    "@anakeen/webpack-conf": "1.2.1",
