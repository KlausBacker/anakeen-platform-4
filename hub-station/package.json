    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-typescript2": "^0.25.3",
    "rollup-plugin-vue": "^5.1.4"
  },
  "peerDependencies": {
    "@anakeen/user-interfaces": "3.0.2-RC.2020.1.21",
    "axios": "0.19.0",
    "jquery": "3.4.1",
    "vue": "2.6.10",
