{
  "name": "@anakeen/hub-components",
  "version": "1.0.1",
  "autorelease": true,
  "description": "Anakeen Hub Components",
  "main": "components/lib/hub-components.common.min.js",
  "scripts": {
    "beautify": "npx eslint ./ --fix",
    "lint": "npx eslint ./",
    "lib:dev": "npx webpack --env development --config ./components/webpack/index.js",
    "lib": "yarn run lib:dev",
    "buildVendor:legacy": "conf=DEV npx webpack --config ./webpack/hubVendor.js",
    "buildVendor:dev": "conf=LEGACY npx webpack --config ./webpack/hubVendor.js",
    "buildVendor": "yarn run buildVendor:dev && yarn run buildVendor:legacy",
    "buildJs:prod": "conf=PROD npx webpack --config ./webpack/index.js",
    "buildJs:dev": "conf=DEV npx webpack --config ./webpack/index.js",
    "buildJs:legacy": "conf=LEGACY npx webpack --config ./webpack/index.js",
    "buildJs": "yarn run buildJs:prod && yarn run buildJs:dev && yarn run buildJs:legacy",
    "buildJsRender:prod": "conf=PROD npx webpack --config ./webpack/render.js",
    "buildJsRender:dev": "conf=DEV npx webpack --config ./webpack/render.js",
    "buildJsRender:legacy": "conf=LEGACY npx webpack --config ./webpack/render.js",
    "buildJsRender": "yarn run buildJsRender:prod && yarn run buildJsRender:dev && yarn run buildJsRender:legacy",
    "watch": "webpack --watch --env development --config ./components/webpack/index.js"
  },
  "files": [
    "/components/**/*",
    "/components/README.md",
    "/src/**/*",
    "yarn.lock"
  ],
  "repository": {
    "type": "git",
    "url": "git@gitlab.anakeen.com:Anakeen/Users/charles.bonnissent/anakeen-hub.git"
  },
  "keywords": [
    "hub",
    "station"
  ],
  "author": "Anakeen",
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "@anakeen/internal-components": "^1.0.1",
    "@progress/kendo-layout-vue-wrapper": "^2019.1.220",
    "@types/navigo": "^7.0.1",
    "@types/url-join": "^4.0.0",
    "axios": "^0.19.0",
    "core-js": "3.1.3",
    "jquery": "^3.3.1",
    "navigo": "^7.1.2",
    "typescript": "^3.3.4000",
    "url-join": "^4.0.0",
    "vue": "2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-gettext": "^2.1.3",
    "vue-property-decorator": "^8.1.0",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@anakeen/anakeen-ci": "^1.0.0",
    "@anakeen/anakeen-cli": "^1.1.13",
    "@anakeen/user-interfaces": "2.0.1",
    "@anakeen/webpack-conf": "^1.2.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@progress/kendo-buttons-vue-wrapper": "^2019.1.220",
    "@progress/kendo-datasource-vue-wrapper": "^2019.1.220",
    "@progress/kendo-dropdowns-vue-wrapper": "^2019.1.220",
    "@progress/kendo-ui": "2019.2.605",
    "@types/node": "^12.0.8",
    "commitlint": "^8.0.0",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "fg-loadcss": "^2.1.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "husky": "^2.4.1",
    "little-loader": "^0.2.0",
    "prettier": "^1.17.0",
    "ts-loader": "^6.0.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "vuebar": "^0.0.20",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  },
  "resolutions": {
    "@progress/kendo-layout-vue-wrapper/vue": "2.6.10"
  },
  "peerDependencies": {
    "@anakeen/internal-components": "^1.0.1",
    "vue": "2.6.10"
  },
  "husky": {
    "hooks": {
      "pre-push": "npx eslint ./ && npx tslint './components/src/**/*.ts' && npx prettier --check './components/src/**/*.ts' && make lint && make checkPackage",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
