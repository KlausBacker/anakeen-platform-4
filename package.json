{
  "name": "@anakeen/hub-components",
  "version": "1.0.0",
  "autorelease": true,
  "description": "Anakeen Hub Components",
  "main": "components/lib/hub-components.common.min.js",
  "scripts": {
    "beautify": "npx eslint ./ --fix",
    "lint": "npx eslint ./",
    "buildJs": "node --max_old_space_size=4096 ./node_modules/.bin/webpack --config webpack/index.js --progress",
    "lib:prod": "webpack --env production --config ./components/webpack/index.js --progress",
    "lib:dev": "webpack --env development --config ./components/webpack/index.js --progress",
    "lib": "npm run lib:prod && npm run lib:dev",
    "watch": "webpack --watch --env development --config ./components/webpack/index.js"
  },
  "files": [
    "/components/lib",
    "/components/README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git@gitlab.anakeen.com:Anakeen/Users/charles.bonnissent/anakeen-hub.git"
  },
  "keywords": [
    "hub",
    "station"
  ],
  "author": "Anakeen",
  "license": "MIT",
  "dependencies": {
    "@anakeen/ank-components": "^1.0.2-dev20190213085409",
    "vue-class-component": "^6.2.0",
    "vue-property-decorator": "^7.3.0"
  },
  "devDependencies": {
    "@anakeen/anakeen-ci": "^1.0.0",
    "@anakeen/anakeen-cli": "^1.0.8-dev20190212111736",
    "@anakeen/webpack-conf": "latest",
    "@commitlint/config-conventional": "^7.3.1",
    "@progress/kendo-layout-vue-wrapper": "^2018.3.1025",
    "@progress/kendo-ui": "2018.2.806",
    "axios": "^0.18.0",
    "commitlint": "^7.3.2",
    "document-register-element": "^1.8.1",
    "easygettext": "^2.4.0",
    "eslint": "^5.12.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-vue": "^5.1.0",
    "husky": "^1.3.1",
    "jsoneditor": "^5.18.0",
    "load-css-file": "^1.0.0",
    "pngquant-bin": "^5.0.0",
    "prettier": "^1.16.4",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "vue": "^2.5.16",
    "vue-custom-element": "^3.0.6",
    "vue-gettext": "^2.0.30",
    "vue-m-loader": "^1.0.1",
    "vue-router": "^3.0.2",
    "vue-router-multi-view": "^0.0.4",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "vuex": "^3.0.1"
  },
  "husky": {
    "hooks": {
      "pre-push": "npx eslint ./ && npx tslint './components/src/**/*.ts' && npx prettier --check './components/src/**/*.ts'",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
