{
  "name": "@anakeen/internal-components",
  "version": "1.0.1",
  "description": "Anakeen internal components",
  "main": "lib/ank-internal-components.common.min.js",
  "typings": "lib/types/index.d.ts",
  "unpkg": "lib/ank-internal-components.umd.min.js",
  "repository": "git@gitlab.anakeen.com:Anakeen/Platform-4/npm/internal-components.git",
  "author": "Anakeen",
  "license": "LicenseRef-LICENSE",
  "scripts": {
    "lint:fix": "npx eslint ./ --fix && npx tslint ./src/**/*.ts --fix",
    "lint": "npx eslint ./ && npx tslint ./src/**/*.ts",
    "prettier": "prettier --check ./src/**/*.{js,ts}",
    "prettier:fix": "prettier --write ./src/**/*.{js,ts}",
    "build:prod": "node --max_old_space_size=4096 node_modules/.bin/webpack --env production --config ./webpack/index.js --progress",
    "build:dev": "node --max_old_space_size=4096 node_modules/.bin/webpack --env development --config ./webpack/index.js --progress",
    "build": "npm run build:prod && npm run build:dev",
    "watch": "webpack --watch --env development --config ./components/webpack/index.js"
  },
  "files": [
    "lib",
    "README.md"
  ],
  "devDependencies": {
    "@anakeen/anakeen-ci": "^1.0.8",
    "@anakeen/webpack-conf": "^1.0.2",
    "@commitlint/config-conventional": "^7.5.0",
    "commitlint": "^7.5.2",
    "eslint": "^5.14.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-vue": "^5.2.2",
    "husky": "^1.3.1",
    "prettier": "^1.16.4",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.6",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@anakeen/user-interfaces": "^2.0.0-dev20190219120819",
    "@progress/kendo-layout-vue-wrapper": "^2019.1.124",
    "vue": "^2.6.6",
    "vue-class-component": "^7.0.1",
    "vue-property-decorator": "^7.3.0"
  },
  "peerDependencies": {
    "vue": "^2.6.6"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm run prettier",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
