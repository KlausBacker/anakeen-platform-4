<?xml version="1.0" encoding="UTF-8"?>
<smart:config xmlns:smart="https://platform.anakeen.com/4/schemas/smart/1.0">

    <smart:enumerates>
        <smart:enum-configuration name="TST_005-colors">
            <smart:enum name="red" label="Rouge"/>
            <smart:enum name="yellow" label="Jaune"/>
            <smart:enum name="green" label="Vert"/>
            <smart:enum name="blue" label="Bleu">
                <smart:enum name="lightblue" label="Bleu ciel"/>
                <smart:enum name="navyblue" label="Bleu marine"/>
            </smart:enum>
        </smart:enum-configuration>
        <smart:enum-configuration name="TST_005-numbers">
            <smart:enum name="1" label="One"/>
            <smart:enum name="2" label="Two"/>
        </smart:enum-configuration>
        <smart:enum-configuration name="TST_005-notes">
            <smart:enum name="A" label="Lab"/>
            <smart:enum name="B" label="Si"/>
        </smart:enum-configuration>

        <smart:enum-configuration name="TST_005-letters">
            <smart:enum-callable function="Anakeen\Pu\Config\EnumTesting::letters"/>
        </smart:enum-configuration>

        <smart:enum-configuration name="TST_005-country">
            <smart:enum-callable function="Anakeen\Pu\Config\EnumExtendsTesting"/>
        </smart:enum-configuration>

    </smart:enumerates>
</smart:config>
