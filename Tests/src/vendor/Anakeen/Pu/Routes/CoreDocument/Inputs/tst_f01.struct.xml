    <smart:structure-configuration name="TST_API_ALLTYPE" label="Test tout type">
        <smart:usefor>N</smart:usefor>
        <smart:fields>
            <smart:field-set name="tst_api__f_title" type="frame" label="Titre" access="ReadWrite">
                <smart:field-text name="tst_api__title" label="Le titre" access="ReadWrite" is-title="true" is-abstract="true"/>
            </smart:field-set>
            <smart:field-set name="tst_api__t_tab" type="tab" label="Basiques" access="ReadWrite">
                <smart:field-set name="tst_api__fr_rels" type="frame" label="Relations" access="ReadWrite">
                    <smart:field-account name="tst_api__account" label="Un compte" access="ReadWrite"/>
                    <smart:field-account name="tst_api__account_multiple" label="Des comptes" access="ReadWrite" multiple="true"/>
                    <smart:field-docid name="tst_api__docid" label="Un document" access="ReadWrite" relation="TST_API_ALLTYPE"/>
                    <smart:field-docid name="tst_api__docid_multiple" label="Des documents" access="ReadWrite" relation="TST_API_ALLTYPE" multiple="true"/>
                </smart:field-set>
                <smart:field-set name="tst_api__fr_date" type="frame" label="Le temps" access="ReadWrite">
                    <smart:field-date name="tst_api__date" label="Une date" access="ReadWrite"/>
                    <smart:field-time name="tst_api__time" label="Une heure" access="ReadWrite"/>
                    <smart:field-timestamp name="tst_api__timestamp" label="Une date avec  une heure" access="ReadWrite"/>
                </smart:field-set>
                <smart:field-set name="tst_api__fr_number" type="frame" label="Les nombres" access="ReadWrite">
                    <smart:field-int name="tst_api__integer" label="Un entier" access="ReadWrite"/>
                    <smart:field-double name="tst_api__double" label="Un décimal" access="ReadWrite"/>
                    <smart:field-money name="tst_api__money" label="Un sous" access="ReadWrite"/>
                </smart:field-set>
                <smart:field-set name="tst_api__fr_misc" type="frame" label="Divers" access="ReadWrite">
                    <smart:field-password name="tst_api__password" label="Un mot de passe" access="ReadWrite"/>
                    <smart:field-color name="tst_api__color" label="Une couleur" access="ReadWrite"/>
                </smart:field-set>
                <smart:field-set name="tst_api__fr_file" type="frame" label="Fichiers &amp; images" access="ReadWrite">
                    <smart:field-file name="tst_api__file" label="Un fichier" access="ReadWrite"/>
                    <smart:field-image name="tst_api__image" label="Une image" access="ReadWrite"/>
                </smart:field-set>
                <smart:field-set name="tst_api__fr_text" type="frame" label="Les textes" access="ReadWrite">
                    <smart:field-htmltext name="tst_api__htmltext" label="Un texte formaté" access="ReadWrite"/>
                    <smart:field-longtext name="tst_api__longtext" label="Un texte multiligne" access="ReadWrite"/>
                    <smart:field-text name="tst_api__text" label="Un texte simple" access="ReadWrite"/>
                </smart:field-set>
                <smart:field-set name="tst_api__fr_enumsimple" type="frame" label="Énumérés directs simple" access="ReadWrite">
                    <smart:field-enum name="tst_api__enumlist" label="Un énuméré liste" access="ReadWrite" relation="TST_API_ALLTYPE-countries">
                        <smart:field-option name="bmenu">no</smart:field-option>
                    </smart:field-enum>
                    <smart:field-enum name="tst_api__enumhorizontal" label="Un énuméré multi-niveau" access="ReadWrite" relation="TST_API_ALLTYPE-colors">
                        <smart:field-option name="bmenu">no</smart:field-option>
                    </smart:field-enum>
                </smart:field-set>
                <smart:field-set name="tst_api__fr_enummultiple" type="frame" label="Énumérés directs multiple" access="ReadWrite">
                    <smart:field-enum name="tst_api__enumslist" label="Des énumérés liste" access="ReadWrite" relation="TST_API_ALLTYPE-countries" multiple="true">
