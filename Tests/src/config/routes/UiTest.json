{
  "apps": {
    "UITEST": {
      "version": "1.0.0",
      "shortName": "List all ui test pages",
      "description": "Interface to view test pages",
      "icon": "apps\/uitest\/uitest.png"
    }
  },
  "accesses": {
    "uitest:user": {
      "applicationContext": "UITEST",
      "description": "Normal Access"
    },
    "uitest:admin": {
      "applicationContext": "UITEST",
      "description": "Administrator Access"
    }
  },
  "routes": {
    "Ui:Test:Page": {
      "priority": 1,
      "callable": "\\Anakeen\\Routes\\UiTest\\TestPage",
      "methods": [
        "GET"
      ],
      "pattern": "\/apps\/uitest\/",
      "description": "Test Ui Page",
      "applicationContext": "UITEST",
      "requiredAccess": "uitest:user"
    },
    "Ui:Test:ExportData": {
      "priority": 1,
      "callable": "\\Anakeen\\Routes\\UiTest\\ExportData",
      "methods": [
        "GET"
      ],
      "pattern": "\/api\/v2\/uitest\/data",
      "description": "Test Ui Export Test Description",
      "requiredAccess": {
        "and": [
          "uitest:admin", "uitest:user"
        ]
      }
    }
  }
}