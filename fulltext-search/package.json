  "repository": "git@gitlab.anakeen.com:Anakeen/Platform-4/fulltext-search.git",
  "keywords": [
    "anakeen",
    "platform",
    "smart",
    "fulltext",
    "search"
  ],
  "files": [
    "components/**/*",
    "/constants/*.js",
    "src/public/Anakeen/manifest/**/*.json",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "beautify": "eslint ./src --fix",
    "buildJs:fsdock:prod": "npx webpack --config webpack/config.js",
    "buildJs:fsdock:dev": "dev=DEV npx webpack --config webpack/config.js",
    "buildJs": "yarn run buildJs:fsdock:prod && yarn run buildJs:fsdock:dev",
