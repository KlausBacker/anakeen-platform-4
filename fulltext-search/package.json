    "buildTest:dev": "conf=DEV npx webpack --config ./webpack/test.js",
    "buildTest": "yarn run buildTest:prod && yarn run buildTest:dev",
    "lib:rollup": "CLEAN=true npx rollup -c ./components/rollup/rollup.config.js",
    "dev": "dev=DEV npx webpack-dev-server --config=./webpack/config.js"
  },
  "devDependencies": {
