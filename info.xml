<?xml version="1.0" ?>
<module xmlns="urn:dynacase:webinst:module/1.0" name="@moduleName@" release="@release@" version="@version@">
    <description>Workflows</description>


    <replaces>

    </replaces>

    <requires>
        <module name="anakeen-core"/>
        <module name="anakeen-security"/>
    </requires>

    <pre-install>

    </pre-install>
    <post-install>
        <process command="./wsh.php --api=importDocuments --file=./vendor/Anakeen/Families/wdoc/wdoc.struct.csv  --csv-enclosure='&quot;'">
            <label>WDOC Family</label>
        </process>
        <process command="./wsh.php --api=importDocuments --file=./vendor/Anakeen/Families/timer/timer.struct.csv  --csv-enclosure='&quot;'">
            <label>TIMER Family</label>
        </process>

        <process command="./wsh.php --api=importDocuments --file=./vendor/Anakeen/Families/wdoc/wdoc.profil.csv  --csv-enclosure='&quot;'">
            <label>WDOC Profil</label>
        </process>
