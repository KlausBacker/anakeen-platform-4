    <post-upgrade>
        <process command="./wsh.php --api=importDocuments --file=./vendor/Anakeen/Families/wdoc/wdoc.struct.csv  --csv-enclosure='&quot;'">
            <label>WDOC Family</label>
        </process>
        <process command="./wsh.php --api=importDocuments --file=./vendor/Anakeen/Families/timer/timer.struct.csv  --csv-enclosure='&quot;'">
