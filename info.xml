<?xml version="1.0" encoding="utf8"?>
<module name="@moduleName@" version="@version@" release="@release@"
        license="http://docs.anakeen.com/licenses/anakeen-3.0/license.html Anakeen License Agreement v3">
    <description lang="en">Transformation Engine Client Library</description>
  <requires>
    <module name="admin-center" />
  </requires>
  <replaces>
    <module name="anakeen-te" />
  </replaces>
  <post-install>
    <process command="programs/record_application TENGINE_CLIENT I">
      <label lang="en">Record application TENGINE_CLIENT in database</label>
    </process>
    <process command="programs/update_catalog"/>
  </post-install>
  <post-upgrade>
    <process command="programs/pre_migration TENGINE_CLIENT"/>
    <process command="programs/record_application TENGINE_CLIENT U">
      <label lang="en">Update application TENGINE_CLIENT in database</label>
    </process>
    <process command="programs/post_migration TENGINE_CLIENT"/>
    <process command="programs/update_catalog"/>
  </post-upgrade>
</module>
